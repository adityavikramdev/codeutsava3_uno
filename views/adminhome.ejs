  <div class="content">
    <center><h1>Admin Authentication</h1></center>
    <br>
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-6">
        <table class="table table-striped table-bordered" style="overflow:hidden;">
          <caption><strong>STARTUPS REGISTERED WITH THE INCUBATOR</strong></caption>
          <thead class="thead-dark">
          <tr>
            <th style="display:none;">id</th>
            <th>S.No</th>
            <th>Registration date</th>
            <th>Startup Name</th>
            <th>Status</th>
          </tr>
        </thead>
          <tbody>
            <!-- <%= result %> -->
            <% result.forEach(function(user){ %>
            <% user.forEach(function(user1){ %>
            <tr>
            <td style="display:none;"><%= user1._id %></td>
            <td>1</td>
            <td><%= user1.date %></td>
            <td><a href="#popup"><%= user1.name %></a></td>
            <td><center><button type="submit" class="btn btn-primary" name="button">Activate</button></center></td>
            
          </tr>
          <% }) %>
          <% }) %> 

          </tbody>
        </table>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-6">
        <table class="table table-striped table-bordered" style="overflow:hidden;">
          <caption><strong>MENTORS REGISTERED WITH THE INCUBATOR</strong></caption>
