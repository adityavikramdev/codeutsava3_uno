  <script>
    $(document).ready(function(){
      $('.pid').on('click',updateStatus)
    });
    function updateStatus(){
      var confirmation=confirm("Are you sure?");
      if(confirmation){
        $.ajax({
          // console.log('user1._id');
          type:"POST",
          url:"/updateprojectstatus"+$('.lid')

        }).done(function(response){
          window.location.replace('/getprojects');
        });

      }else{
        return false;
      }
    }
  </script>
