        <section id="funds_form">
              <div class="container">
                  <div class="row">
                    <div class="col-lg-offset-2 col-lg-8">
                    <div class="panel panel-primary">
                    <div class="panel-heading"><strong>Startup Registration Form</strong></div>
                    <div class="panel-body">

                      <form method="post" action="/startupregister">
                        <div class="form-group">
                          <label for="text">Name of Startup:</label>
                          <input type="text" class="form-control" required="true" name="name" id="sto">
                        </div>
                        <div class="form-group">
                          <label for="reg">Founder/Co-Founder Name:</label>
                          <input type="text" class="form-control" required="true" name="headName" id="reg">
                        </div>
                        <div class="form-group">
                          <label for="text">Email Id:</label>
                          <input type="email" class="form-control" required="true" name="email" id="sto">
                        </div>
                        <div class="form-group">
                          <label for="text">DIPP NO:</label>
                          <input type="text" class="form-control" required="true" name="dippNumber" id="sto">
                        </div>
                        <div class="form-group">
                          <label for="reg">Contact No:</label>
                          <input type="text" class="form-control" required="true" name="contactNumber" id="reg">
                        </div>
                        <div class="form-group">
                          <label for="text">Password:</label>
                          <input type="password" class="form-control" required="true" name="password" id="sto">
                        </div>
                        <div class="form-group">
                          <label for="reg">Confirm Password:</label>
                          <input type="password" class="form-control" id="reg">
                        </div>
                        <div class="form-group">
                          <label for="text">Address:</label>
                          <input type="text" class="form-control" required="true" name="address" id="sto">
                        </div>
                        <div class="form-group">
                          <label for="text">Affliated By:</label>&nbsp;
                          <input type="radio" name="affil" value="none" checked>&nbsp;None &nbsp;
                          <input type="radio" name="affil" value="Central Government">&nbsp;Central Government &nbsp;
                          <input type="radio" name="affil" value="State Government">&nbsp;State Government &nbsp;
                          <input type="radio" name="affil" value="Incubation Center">&nbsp;Incubation center &nbsp;
                        </div>
                        <button type="submit" class="btn btn-primary">Submit</button>
                      </form>
                    </div>
                    </div>

